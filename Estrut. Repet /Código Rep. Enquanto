programa
{
	
	funcao inicio()
	{
		inteiro numero, contpositivo = 0, contnegativo = 0, qtd = 0
		enquanto (qtd < 10){
			escreva("Digite um numero: ")
			leia(numero)

			se(numero > 0){
			contpositivo++
			}senao{
			contnegativo++
			}
			qtd++
	}
			escreva ("Quantidade de numeros positivos é ", contpositivo)
			escreva ("\nQuantidade de numeros negativos é ", contnegativo)
		}
}

