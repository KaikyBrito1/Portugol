programa
{
	
	funcao inicio()
	{
		inteiro codigo, idade, identidade
		cadeia tiposang
		escreva("Digite seu codigo de funcionario: ")
		leia(codigo)
		escreva("Digite sua idade: ")
		leia(idade)
		escreva("Digite seu tipo sanguineo: ")
		leia(tiposang)
		escreva("Digite o numero da sua identidade: ")
		leia(identidade)

		escreva("\nSeu codigo de funcionario é: ", codigo, "\nSua idade é: ",idade , "\nSeu tipo sanguineo é: ",tiposang, "\nNumero da identidade é :", identidade)
		

			}
		}
	

