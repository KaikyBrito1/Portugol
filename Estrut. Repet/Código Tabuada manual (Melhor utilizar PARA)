programa
{
	
	funcao inicio()
	{
	inteiro numero
	escreva("Digite um numero: ")
	leia(numero)

	escreva(numero * 1, ",")
	escreva(numero * 2, ",")
	escreva(numero * 3, ",")
	escreva(numero * 4, ",")
	escreva(numero * 5, ",")
	escreva(numero * 6, ",")
	escreva(numero * 7, ",")
	escreva(numero * 8, ",")
	escreva(numero * 9, ",")
	escreva(numero * 10, ",")
	
	
		
	}
}
